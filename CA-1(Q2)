ORG 0000H

MOV R0, 50H
MOV R1, 51H

LOOP1:
MOV A, R1
JZ B_ZERO1

DEC R0
DEC R1

ORL A, #01H
JNZ LOOP1

B_ZERO1:
MOV 52H, #01H   ; A > B

END




ORG 0000H
MOV R0, 50H
MOV R1, 51H

LOOP2:
MOV A, R0
JZ A_ZERO2

DEC R0
DEC R1

ORL A, #01H
JNZ LOOP2

A_ZERO2:
MOV A, R1
JZ EQUAL2

EQUAL2:
MOV 52H, #00H   ; A = B

END




ORG 0000H

MOV R0, 50H
MOV R1, 51H

LOOP3:
MOV A, R0
JZ A_ZERO3

DEC R0
DEC R1

ORL A, #01H
JNZ LOOP3

A_ZERO3:
MOV 52H, #0FFH  ; A < B

END



